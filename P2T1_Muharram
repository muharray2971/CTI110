import matplotlib.pyplot as plt

# collect the data 
data = [] # empty list 
print (" enter pokeman data;")
print("day 1:", end="")
day1 = int(input())
print("day 2:", end="")
day2 = int(input())
print("day 3:", end="")
day3 = int(input())

# put the data in a list
data = [day1, day2, day3]

# TODo: graph the real dat 
fig, ax = plt.subplots()
ax.plot([1, 2, 3], data)
plt.title("pokeman data")
plt.xlabel('day #')
plt.ylabel('pokrmans collected')
plt.show()
